{
    "manifest_version": 3,
    "version": "0.0.0.1",
    "name": "Porównanie produktów na x-kom.pl",
    "description": "Zwiększ ilość porównywanych produktów na x-kom.pl",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "content_scripts": [
        {
            "js": [
                "main.js"
            ],
            "matches": [
                "https://www.x-kom.pl/porownanie/*"
            ]
        }
    ]
}
